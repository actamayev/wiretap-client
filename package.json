{
  "name": "wiretap-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sudo next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run lint && pnpm run type-check",
    "analyze": "ANALYZE=true next build",
    "postinstall": "next telemetry disable",
    "check-unused": "ts-unused-exports tsconfig.json --excludePathsFromReport=\"test|spec|\\.next|proxy\\.ts|app/(manifest|robots|sitemap).ts|app/(layout|not-found).tsx|src/components/ui|app/.*/page\\.tsx\""
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "ogl": "^1.0.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0",
    "react-ga4": "^2.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "eslint-plugin-react": "^7.37.5",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "@typescript-eslint/parser": "^8.46.3"
  }
}
